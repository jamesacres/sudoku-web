{
  "name": "sudoku-web",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.mjs",
  "author": "James Acres",
  "description": "Sudoku",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "electron": "electron .",
