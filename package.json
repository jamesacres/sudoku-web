{
  "name": "sudoku-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.8.1",
  "author": "James Acres",
  "description": "Sudoku Race",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:sudoku": "turbo run dev --filter=@sudoku-web/app-sudoku",
    "dev:template": "turbo run dev --filter=@sudoku-web/app-template",
    "build": "turbo run build",
    "build:sudoku": "turbo run build --filter=@sudoku-web/app-sudoku",
    "build:sudoku:capacitor": ". ./scripts/buildCapacitor.sh sudoku",
    "build:sudoku:electron": "./scripts/buildElectron.sh sudoku",
    "build:template": "turbo run build --filter=@sudoku-web/app-template",
    "build:template:capacitor": ". ./scripts/buildCapacitor.sh template",
    "build:template:electron": "./scripts/buildElectron.sh template",
    "type-check": "turbo run type-check",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "test": "turbo run test",
    "test:watch": "turbo run test -- --watch",
    "test:coverage": "turbo run test -- --coverage",
    "test:ci": "turbo run test -- --coverage --ci --maxWorkers=2",
    "generate:icons": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor-community/in-app-review": "^7.0.1",
    "@capacitor/android": "^7.0.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/browser": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/share": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@headlessui/react": "^1.7.19",
    "@revenuecat/purchases-capacitor": "^11.2.6",
    "@revenuecat/purchases-js": "^1.9.0",
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-backend-wasm": "^4.20.0",
    "capacitor-secure-storage-plugin": "^0.11.0",
    "mathjs": "^13.0.2",
    "next": "^14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-feather": "^2.0.10"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "electron-store": "^9.0.0",
    "eslint": "^8",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "strict-event-emitter-types": "^2.0.0",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.1.1",
    "turbo": "^2.6.0",
    "typescript": "^5"
  }
}