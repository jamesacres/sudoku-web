{
  "name": "sudoku-web",
  "version": "0.1.0",
  "private": true,
  "author": "James Acres",
  "description": "Sudoku Race",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:capacitor": ". ./scripts/buildCapacitor.sh",
    "build:electron": "./scripts/buildElectron.sh",
    "open:android": "npm run build:capacitor && npx cap open android",
    "open:ios": "npm run build:capacitor && npx cap open ios",
    "start": "npm run build && next start",
    "start:android": "npm run build:capacitor && npx cap run android",
    "start:ios": "npm run build:capacitor && npx cap run ios",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "generate:icons": "npx @capacitor/assets generate --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/browser": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/share": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@headlessui/react": "^1.7.19",
    "@revenuecat/purchases-capacitor": "^11.0.0",
    "@revenuecat/purchases-js": "^1.9.0",
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-backend-wasm": "^4.20.0",
    "capacitor-secure-storage-plugin": "^0.11.0",
    "mathjs": "^13.0.2",
    "next": "^14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-feather": "^2.0.10"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "electron-store": "^9.0.0",
    "eslint": "^8",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "strict-event-emitter-types": "^2.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5"
  }
}
