{
  "name": "@sudoku-web/auth",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./components/*": "./src/components/*.tsx",
    "./providers/*": "./src/providers/*.tsx",
    "./hooks/*": "./src/hooks/*.ts",
    "./helpers/*": "./src/helpers/*.ts",
    "./config/*": "./src/config/*.ts",
    "./services/*": "./src/services/*.ts",
    "./types/*": "./src/types/*.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "scripts": {
    "build": "echo 'Auth package is pure TypeScript - no compilation needed'",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/react": "^18",
    "@types/react-dom": "^18"
  }
}