    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5"
